
<div class="container">
  <div class="row">
    <%= form_for @project do |f| %>
      <%= f.label :project_url %>:
      <%= f.text_field :proposal_url %>
      <%= f.submit %>
    <% end %>
  </div>


  <div class="row">
    <h2>Project list</h2>

    <% @projects.each do |p| %>
      <%= link_to p.title, project_path(p) %>
    <% end %>
  </div>
</div>
