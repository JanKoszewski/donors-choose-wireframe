<div class="challenge-nav-tab-contents">
  <a class="nav-tab-link" href="#tab<%=challenge.id%>" data-toggle="tab"></a>
<div class="row any-width">
  <div class="twocol">
    <div class="at-price">At <%= humanized_money_with_symbol challenge.target_funding %></div>
  </div>
  <div class="twocol">
    <a class="nav-tab-link" href="#tab<%=challenge.id%>" data-toggle="tab">
      <%= image_tag(challenge.display_media) %>
    </a>
  </div>
  <div class="sixcol last">
    <span class="challenge-nav-text"><span class="challenge-username"><%= challenge.name %></span> will: <%= challenge.title %></span>
    <div>
      <%= link_to("View Challenge", project_challenge_path(@project, challenge), class: "challenge-link")  %>
    </div>
  </div>
</div>
</div>
